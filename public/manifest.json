{
    "manifest_version": 3,
    "default_locale": "en",
    "name": "__MSG_extensionName__",
    "version": "0.0.1",
    "description": "__MSG_extensionDescription__",
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "action": {
      "default_icon": "icon-34.png"
    },
    "icons": {
      "128": "icon-128.png"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "*.js",
          "*.css",
          "*.svg",
          "icon-128.png",
          "icon-34.png"
        ],
        "matches": [
          "*://*/*"
        ]
      }
    ]
  }